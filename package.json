    "@typescript-eslint/parser": "^6.19.0",
    "commitlint": "^18.4.4",
    "debug": "^4.3.4",
    "depcheck": "^1.4.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "nsp": "^3.2.1",
